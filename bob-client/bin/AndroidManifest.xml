<?xml version='1.0' encoding='utf-8'?>
<manifest package="com.protogenefactory.ioiomaster" android:versionName="0.6" android:versionCode="6" xmlns:android="http://schemas.android.com/apk/res/android">

    
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.RECORD_AUDIO"/>
    <uses-feature android:name="android.hardware.camera"/>
    <uses-feature android:name="android.hardware.camera.autofocus"/>

	
    <uses-sdk android:targetSdkVersion="19" android:minSdkVersion="14"/>

    <application android:theme="@style/AppTheme" android:name="com.protogenefactory.ioiomaster.client.BobApplication" android:label="@string/app_name" android:icon="@drawable/ic_launcher" android:allowBackup="true">

        
        <activity android:name="com.protogenefactory.ioiomaster.client.activities.ProjectListActivity" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name="com.protogenefactory.ioiomaster.client.activities.ProjectActivity"/>

        <activity android:name="com.protogenefactory.ioiomaster.client.activities.NewProjectActivity" android:label="New project"/>

        <activity android:name="com.protogenefactory.ioiomaster.client.activities.BoardConfigActivity" android:label="New board configuration"/>


        
        <service android:name="net.majorkernelpanic.streaming.rtsp.RtspServer"/>

        <activity android:name="com.protogenefactory.ioiomaster.server.VideoActivity" android:label="Remote video">
        </activity>

    <uses-library android:name="android.test.runner"/></application>

<instrumentation android:name="android.test.InstrumentationTestRunner" android:label="Test Runner" android:targetPackage="com.protogenefactory.ioiomaster"/></manifest>